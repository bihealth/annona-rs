syntax = "proto3";

package annonars.cons.pbs;

// Protocol buffer for the UCSC conservation record.
message Record {
    // Chromosome name.
    string chrom = 1;
    // 1-based, inclusive start position.
    int32 start = 2;
    // 1-based, inclusive stop position.
    int32 stop = 3;
    // HGNC identifier.
    string hgnc_id = 4;
    // ENST identifier.
    string enst_id = 5;
    // Alignment.
    string alignment = 6;
}
